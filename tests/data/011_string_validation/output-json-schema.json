{
    "$$version": 1,
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "title": "String validation",
    "$$order": [
        "ssid",
        "password"
    ],
    "required": [
        "ssid",
        "password"
    ],
    "properties": {
        "ssid": {
            "type": "string",
            "pattern": "^[a-zA-Z]{1,}$"
        },
        "password": {
            "type": "string",
            "minLength": 8,
            "maxLength": 32,
            "writeOnly": true
        }
    }
}
