{
    "$$version": 1,
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "title": "Allow additional properties in the data.",
    "additionalProperties": true,
    "$$order": [
        "property"
    ],
    "required": [
        "property"
    ],
    "properties": {
        "property": {
            "type": "object",
            "additionalProperties": true,
            "properties": {
                "subproperty": {
                    "type": "string"
                }
            },
            "$$order": [
                "subproperty"
            ],
            "required": [
                "subproperty"
            ]
        }
    }
}
