{
    "$$version": 1,
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "title": "Types with same definitions as in JSONSchema",
    "$$order": [
        "string",
        "datetime",
        "date",
        "time",
        "hostname",
        "email",
        "ipv4",
        "ipv6",
        "uri",
        "dnsmasq-address",
        "chrony-address",
        "iptables-address"
