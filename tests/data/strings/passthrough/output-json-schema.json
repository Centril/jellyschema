            "format": "date-time",
            "default": "1970-01-01T12:00:00.000Z"
        },
        "date": {
            "type": "string",
            "format": "date",
            "default": "1970-01-01"
        },
        "time": {
            "type": "string",
            "format": "time",
            "default": "12:00"
        },
        "hostname": {
            "type": "string",
            "format": "hostname",
            "default": "example.com"
        },
        "email": {
            "type": "string",
            "format": "email",
            "default": "hello@example.com"
        },
        "ipv4": {
            "type": "string",
            "format": "ipv4",
            "default": "1.1.1.1"
        },
        "ipv6": {
            "type": "string",
            "format": "ipv6",
            "default": "2606:4700:4700::1111"
