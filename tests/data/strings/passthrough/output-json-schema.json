{
    "$$version": 1,
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
"additionalProperties": false,
    "title": "Types with same definitions as in JSONSchema",
    "$$order": [
        "string",
        "datetime",
        "date",
        "time",
        "hostname",
        "email",
        "ipv4",
        "ipv6",
        "uri",
        "dnsmasq-address",
        "chrony-address",
        "iptables-address"
    ],
    "required": [
        "string",
        "datetime",
        "date",
        "time",
        "hostname",
        "email",
        "ipv4",
        "ipv6",
        "uri",
        "dnsmasq-address",
        "chrony-address",
        "iptables-address"
    ],
    "properties": {
        "string": {
            "type": "string",
            "default": "Hello world !"
        },
        "datetime": {
            "type": "string",
            "format": "date-time",
            "default": "1970-01-01T12:00:00.000Z"
        },
        "date": {
            "type": "string",
            "format": "date",
            "default": "1970-01-01"
        },
        "time": {
            "type": "string",
            "format": "time",
            "default": "12:00"
        },
        "hostname": {
            "type": "string",
            "format": "hostname",
            "default": "example.com"
        },
        "email": {
            "type": "string",
            "format": "email",
            "default": "hello@example.com"
        },
        "ipv4": {
            "type": "string",
            "format": "ipv4",
            "default": "1.1.1.1"
        },
        "ipv6": {
            "type": "string",
            "format": "ipv6",
            "default": "2606:4700:4700::1111"
        },
        "uri": {
            "type": "string",
            "format": "uri",
            "default": "https://example.com"
        },
        "dnsmasq-address": {
            "type": "string",
            "format": "dnsmasq-address",
            "default": "1.1.1.1"
        },
        "chrony-address": {
            "type": "string",
            "format": "chrony-address",
            "default": "1.1.1.1"
        },
        "iptables-address": {
            "type": "string",
            "format": "iptables-address",
            "default": "1.1.1.1"
        }
    }
}
