{
    "$$version": 1,
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": false,
    "title": "Default values for numbers and integers",
    "$$order": [
        "number",
        "integer",
        "port"
    ],
    "required": [
        "number",
        "integer",
        "port"
    ],
    "properties": {
        "number": {
            "type": "number",
            "default": -7
        },
        "integer": {
            "type": "integer",
            "default": 3
        },
        "port": {
            "type": "integer",
            "minimum": 0,
            "maximum": 65535,
            "default": 80
        }
    }
}
